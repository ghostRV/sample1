<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.DeevRV.sample1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample1">
        <activity android:name=".ThreeThreeBottom"
            android:parentActivityName=".ThreeBottom">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ThreeBottom"/>
            ></activity>
        <activity android:name=".TwoTwoBottom"
            android:parentActivityName=".TwoBottom">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TwoBottom"/>
        </activity>
        <activity android:name=".OneOneBottom" />
        <activity android:name=".ThreeBottom" />
        <activity android:name=".TwoBottom" />
        <activity android:name=".OneBottom" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>